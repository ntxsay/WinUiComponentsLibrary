<StackPanel
    x:Class="WinUiComponentsLibrary.Views.IconableHeader"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:WinUiComponentsLibrary.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" Orientation="Horizontal" Spacing="8">
    <FontIcon x:Name="MyFontIcon" FontFamily="Segoe MDL2 Assets" FontSize="20"/>
    <TextBlock x:Name="TbcTitle" FontSize="16" VerticalAlignment="Center" Margin="0 0 0 5" />
</StackPanel>
